<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    xmlns:font="http://schemas.android.com/apk/res/com.stylist"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="50dp"
    android:background="@color/transparent"
    android:orientation="vertical"
    tools:context="com.stylist.Selector"
    tools:ignore="MergeRootFrame" >

    <RelativeLayout
        android:id="@+id/uploadRelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:background="@color/transparent"
        android:gravity="center_horizontal"
        android:orientation="vertical" >

      <!--   <View
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="40dp"
            android:alpha="0.4"
            android:background="@color/gray" /> -->
        <!--
        <LinearLayout
            android:id="@+id/uploadLinearLayoutt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/uploadLinearLay"
            android:layout_marginBottom="40dp"
            android:layout_marginTop="50dp"
            android:alpha="0.4"
            android:background="@color/gray"
            android:orientation="vertical" >
        </LinearLayout>
        -->

        <LinearLayout
            android:id="@+id/uploadLinearLay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="20dp"
            android:layout_marginLeft="@dimen/fifteen_dp"
            android:layout_marginRight="@dimen/fifteen_dp"
            android:background="@drawable/white_ract"
            android:gravity="bottom|center_horizontal"
            android:orientation="vertical"
            android:paddingBottom="10dp"
            android:paddingTop="10dp" >

            <com.constants.CustomTextView
                android:id="@+id/wardrobe"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:paddingBottom="@dimen/ten_dp"
                android:text="Browse My Wardrobe"
                android:textColor="@drawable/text_selector"
                android:textSize="@dimen/textNormal"
                font:customFonttv="lvnm.ttf" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray" />

            <com.constants.CustomTextView
                android:id="@+id/wishlist"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/transparent"
                android:gravity="center"
                android:padding="@dimen/ten_dp"
                android:text="Browse My Wishlist"
                android:textColor="@drawable/text_selector"
                android:textSize="@dimen/textNormal"
                font:customFonttv="lvnm.ttf" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray" />

            <com.constants.CustomTextView
                android:id="@+id/back"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/transparent"
                android:gravity="center"
                android:paddingTop="@dimen/ten_dp"
                android:text="Cancel"
                android:textColor="@drawable/text_selector"
                android:textSize="@dimen/textNormal"
                font:customFonttv="lvnm.ttf" />

        </LinearLayout>
    </RelativeLayout>

</RelativeLayout>