<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:tools="http://schemas.android.com/tools"
    xmlns:font="http://schemas.android.com/apk/res/com.stylist"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:descendantFocusability="blocksDescendants"
    android:orientation="vertical"
    android:padding="8dip" >
<!-- 
    <ImageView
        android:id="@+id/img"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
     
         /> -->
         <FrameLayout
              android:id="@+id/img"
             android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp">

             <!--<ImageView
                 android:id="@+id/imageWebView"
                 android:layout_width="match_parent"
                 android:layout_height="match_parent"
                 android:focusable="false"
                 android:src="@drawable/cnn"
                 android:focusableInTouchMode="false"/>-->

        <WebView
    android:id="@+id/imageWebView" 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
            android:focusable="false"
            android:focusableInTouchMode="false"/>
</FrameLayout>

    <com.constants.CustomTextView
        android:id="@+id/page_url"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="iiii"
        android:visibility="gone"
        font:customFonttv="lvnm.ttf" />

    <!-- Article title -->

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/img"
        android:orientation="vertical" >

        <com.constants.CustomTextView
            android:id="@+id/title"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="1dip"
            android:text="iiii"
            android:textColor="#303030"
            android:textSize="18dip"
            android:textStyle="bold"
            font:customFonttv="lvnm.ttf" />

        <!-- published date -->

        <com.constants.CustomTextView
            android:id="@+id/pub_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="3dip"
            android:text="iiii"
            android:textColor="#b70400"
            android:textSize="14dip"
            font:customFonttv="lvnm.ttf" />

        <!-- article description -->

        <com.constants.CustomTextView
            android:id="@+id/link"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="6dip"
            android:text="iiii"
            android:textColor="#8d8d8d"
            android:textSize="15dip"
            font:customFonttv="lvnm.ttf" />
    </LinearLayout>

</RelativeLayout>