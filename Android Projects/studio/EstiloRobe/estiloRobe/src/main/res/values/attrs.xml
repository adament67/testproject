<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomTextView">
        <attr name="customFonttv" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomEditText">
        <attr name="customFontet" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomButton">
        <attr name="customFontbtn" format="string" />
    </declare-styleable>
<declare-styleable name="CustomRadioButton">
        <attr name="customFontrbtn" format="string" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>    

    <declare-styleable name="SingleFingerView">
        <attr name="image" format="reference"/>
        <attr name="image_width" format="dimension"/>
        <attr name="image_height" format="dimension"/>
        <attr name="push_image" format="reference"/>
        <attr name="push_image_width" format="dimension"/>
        <attr name="push_image_height" format="dimension"/>
        <attr name="push_image1" format="reference"/>
         <attr name="push_image_width1" format="dimension"/>
        <attr name="push_image_height1" format="dimension"/>
        <attr name="top" format="dimension"/>
        <attr name="left" format="dimension"/>
        <attr name="centerInParent" format="boolean"/>
    </declare-styleable>

</resources>